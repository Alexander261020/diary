<%= render @cargo_order %>

<div>
  <%= link_to "Edit this order", edit_cargo_order_path(@cargo_order), class: "btn btn-success my-2" %>
  <%= link_to "Back to orders", cargo_orders_path, class: "btn btn-success my-2" %>

  <%= button_to "Destroy this order", @cargo_order, method: :delete,
    onclick: "return confirm('Are you sure?')",
    class: "btn btn-danger" %>
</div>
