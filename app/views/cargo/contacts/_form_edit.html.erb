<%= form_with(model: cargo_contact) do |form| %>
  <div>
    <%= form.label :name_contact, style: "display: block" %>
    <%= form.text_field :name_contact %>
  </div>

  <div>
    <%= form.label :tel, style: "display: block" %>
    <%= form.text_field :tel %>
  </div>

  <div>
    <%= form.label :email, style: "display: block" %>
    <%= form.text_field :email %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
